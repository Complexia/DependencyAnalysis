service has two local variables, both dependant on each other. has some outputs connected as well
<?xml version="1.0"?>
<company>
	<service serviceName="A1">
		<serviceName>A1</serviceName>
		<inputService></inputService>
		<outputService>data1, data2, data3, data4</outputService>
		<nameOfVariable>st1, st2</nameOfVariable>
		<inputVariable></inputVariable>
		<outputVariable></outputVariable>
	</service>

	<IOVariables>
		<IOVariable>
			<IOName>st1</IOName>
			<inputs>st2</inputs>
			<outputs>data1, data2, st2</outputs>
		</IOvariable>
		<IOVariable>
			<IOName>data1</IOName>
			<inputs>st1</inputs>
			<outputs></outputs>
		</IOvariable>
		<IOVariable>
			<IOName>data2</IOName>
			<inputs>st1, st2</inputs>
			<outputs></outputs>
		</IOvariable>
		<IOVariable>
			<IOName>data3</IOName>
			<inputs>st2</inputs>
			<outputs></outputs>
		</IOvariable>
		<IOVariable>
			<IOName>data4</IOName>
			<inputs></inputs>
			<outputs></outputs>
		</IOvariable>
		<IOVariable>
			<IOName>st2</IOName>
			<inputs>st1</inputs>
			<outputs>data2, data3, st3</outputs>
		</IOvariable>
	</IOVariables>		
</company>